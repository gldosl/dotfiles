[alias]
    diff-branches = "!f() { git diff $1:$3 $2:$3; }; f"
    branches = for-each-ref --sort=-committerdate refs/heads/ --format='%(color:yellow)%(committerdate:short)%(color:reset) %(refname:short)'
    logline = log --decorate --oneline --graph
